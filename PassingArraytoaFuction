
template<typename arr_type, const int size>
inline int numof(const arr_type (&IdontKnow)[size]) //IdontKnow is a variable name ,which here is an array name
{
    return size;  //when creating function from template , the two types of info have been created .
}

int main()
{
    int a[] = {1,2,3,4,45,5,12,1,2,323,43,23,12,1};
    int c = numof(a);
    return 0 ;
}


